@{
    ViewData["Title"] = "Countrylist";
}

<h1>Countrylist</h1>
<div class="mb-5">
    <table class="table  table-hover table-striped table-bordered">
        <thead class="thead-dark">
            <tr>
                <th>Index</th>
                <th>Name</th>
            </tr>
            </>
        <tbody id="result"></tbody>
    </table>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script>

    $(document).ready(function () {

        $.ajax({
            url: "https://restcountries.com/v3.1/all", /*"https://restcountries.eu/rest/v1/all", "https://restcountries.com/v3.1/all",*/
            type: "get",
            success: function (data) {
                var row = "";
                for (i in data) {
                    console.log(data[i].name)
                    row += "<tr>";
                    row += "<td>" + i + "</td>";
                    row += "<td>" + data[i].name.common + "</td>";
                    row += "<tr>";
                }
                document.getElementById("result").innerHTML = row
            },
        })

    });
</script>

